#include <stdio.h>
//Vivesti naimenshee AND naibolshee

int main(){
  int a,b,c,max,min;
  printf("Write numbers:\n");
  scanf("%d\n %d\n %d", &a,&b,&c);
if (a>b && a>c){
  max=a;
}
else {
  if(b>a && b>c){
    max= b;
  }
  else{
    max=c;
  }
}
if (a<b && a<c){
  min=a;
}
else{
  if (b<a && b<c){
    min=b;
  }
  else{
    min=c;
  }
}
printf("Max=%d\n", max);
printf("Min=%d", min);
}
